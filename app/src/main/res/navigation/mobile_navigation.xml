<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_register">

    <fragment
        android:id="@+id/navigation_register"
        android:name="com.example.farmercrowdfunding.ui.register.register"
        android:label="@string/title_register"
        tools:layout="@layout/register_fragment" />

    <fragment
        android:id="@+id/navigation_scheme"
        android:name="com.example.farmercrowdfunding.ui.scheme.scheme"
        android:label="@string/title_scheme"
