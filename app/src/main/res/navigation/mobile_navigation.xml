    <fragment
        android:id="@+id/navigation_farmerdetails"
        android:name="com.example.farmercrowdfunding.ui.farmerdetails.farmerdetails"
        android:label="@string/title_farmerdetails"
        tools:layout="@layout/farmerdetails_fragment" />
    <fragment
        android:id="@+id/navigation_cropdetails"
